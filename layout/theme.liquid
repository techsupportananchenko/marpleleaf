    </div>

{% endif %} 
  

  

<!-- FOOTER -->
<footer>
<div class="container">
    <div class="row">

        <div class="col-sm-2 col-xs-3 footer_block footer_block__1 wow">
            <h2>{{ settings.footer_block1_title }}</h2>
            <ul>
                {% for link in linklists[settings.footer_block1_list].links limit:7 %}
                <li {% if link.active %}class="active"{% endif %}>{{ link.title | link_to: link.url }}</li>
                {% endfor %}
            </ul>
        </div>

        <div class="col-sm-2 col-xs-3 footer_block footer_block__2 wow">
            <h2>{{ settings.footer_block2_title }}</h2>
            <ul>
                {% for link in linklists[settings.footer_block2_list].links limit:7 %}
                <li {% if link.active %}class="active"{% endif %}>{{ link.title | link_to: link.url }}</li>
                {% endfor %}
            </ul>
        </div>

        <div class="col-sm-2 col-xs-3 footer_block footer_block__3 wow">
            <h2>{{ settings.footer_block3_title }}</h2>
            <ul>
                {% for link in linklists[settings.footer_block3_list].links limit:7 %}
                <li {% if link.active %}class="active"{% endif %}>{{ link.title | link_to: link.url }}</li>
                {% endfor %}
            </ul>
        </div>

        <div class="col-sm-2 col-xs-3 footer_block footer_block__4 wow">
            <h2>{{ settings.footer_block4_title }}</h2>
            <ul>
                
                {% if settings.social_facebook != blank %}<li><a href="{{ settings.social_facebook }}"><span>Facebook</span></a></li>{% endif %}
                
                {% if settings.social_instagram != blank %}<li><a href="{{ settings.social_instagram }}"><span>Instagram</span></a></li>{% endif %}
          
                {% if settings.social_googleplus != blank %}<li><a href="{{ settings.social_googleplus }}"><span>Google+</span></a></li>{% endif %}
              
            </ul>
        </div>          

        <div class="col-sm-4 col-xs-12 footer_block footer_block__5 wow">
            <h2>{{ settings.footer_block5_title }}</h2>
            {% if settings.contacts_company != blank %}<span class="contacts_company">{{ settings.contacts_company }}</span>{% endif %}{% if settings.contacts_address != blank %}<div class="contacts_address">{{ settings.contacts_address }}</div>{% endif %}
            {% if settings.contacts_phone != blank %}
          <p class="contacts_phone"> {{ settings.contacts_phone_text }}<span>{{ settings.contacts_phone }}</span></p>{% endif %}
            {% if settings.contacts_email != blank %}<div class="contacts_email"> <span>{{ settings.contacts_email_text }}</span><a href="mailto:{{ settings.contacts_email }}">{{ settings.contacts_email }}</a></div>{% endif %}
        </div>
      
         <ul class="header_user for-mobile">
    {% if customer %}
    <li><a href="/account"><i class="fa fa-user"></i>{{ customer.first_name | truncate:15 }}</a>    </li>
    <li>{{ 'Log out' | customer_logout_link }}</li>
    {% else %}
        <li>{{ 'Log in' | customer_login_link }}</li>
        {% if shop.customer_accounts_optional %}
        <li>{{ 'Create an account' | customer_register_link }}</li>
        {% endif %}
    {% endif %}
    </ul>

    </div>
  

    

</div>
  {% if template == 'index' %}
   <div class="wow2 container">
        <div class="title3 bord">
          	<span class="wrap6">{{ settings.custom_bottom_6_text1 }}</span>
        </div>
        
     
      
    </div>

{% endif %} 
  
  
  <div class="container">
  <div class="copyright">
        <p role="contentinfo">&copy; {{ "now" | date: "%Y" }} {{ shop.name }}. All Rights Reserved. {% if settings.show_copyright %}Design by <a href="//templatemonster.com">TemplateMonster.com</a>.{% endif %} <br/> {{ powered_by_link }}.</p><!-- Design by templatemonster.com -->
    </div>
  </div>
</footer>
 

    
  

</div><!-- / #wrapper2 -->
</div><!-- / #wrapper -->

